    "lint": "next lint"
  },
  "dependencies": {
    "next": "11.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "eslint": "8.4.1",
    "eslint-config-adjunct": "^4.11.1",
    "eslint-config-next": "12.0.7",
    "typescript": "4.5.2"
  }
}
