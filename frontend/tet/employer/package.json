{
  "name": "@frontend/te-empl",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pre-commit": "lint-staged -c ../../.lintstagedrc.js",
    "test": "jest --runInBand --no-cache --passWithNoTests",
