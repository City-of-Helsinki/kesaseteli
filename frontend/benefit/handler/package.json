    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.13.2",
    "camelcase-keys": "^7.0.0",
    "dotenv": "^9.0.0",
    "esm": "^3.2.25",
    "formik": "^2.2.9",
    "graphql": "^15.5.0",
    "hds-react": "^1.6.0",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.0",
