{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "kesaseteli/employer/*": ["kesaseteli/employer/src/*", "kesaseteli/employer/browser-tests/*"],
      "kesaseteli/handler/*": ["kesaseteli/handler/src/*", "kesaseteli/handler/browser-tests/*"],
      "kesaseteli/youth/*": ["kesaseteli/youth/src/*", "kesaseteli/youth/browser-tests/*"],
      "kesaseteli-shared/*": ["kesaseteli/shared/src/*"],
      "benefit/applicant/*": ["benefit/applicant/src/*"],
      "benefit/handler/*": ["benefit/handler/src/*"],
      "benefit-shared/*": ["benefit/shared/src/*"],
      "tet/admin/*": ["tet/admin/src/*"],
      "tet/employer/*": ["tet/employer/src/*"],
      "tet/youth/*": ["tet/youth/src/*"],
      "shared/*": ["shared/src/*", "shared/browser-tests/*"]
    }
  },
  "exclude": ["node_modules"]
}
