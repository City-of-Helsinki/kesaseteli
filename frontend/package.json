{
  "name": "frontend",
  "version": "1.0.0",
  "main": " ",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "kesaseteli/*",
    "benefit/*",
    "shared"
  ],
  "scripts": {
    "audit": "npx lerna-audit --level=high --groups=dependencies",
    "ks-empl:build": "npx lerna run --scope @frontend/employer --stream build",
    "bf-applicant:build": "npx lerna run --scope @frontend/applicant --stream build",
    "bf-handler:build": "npx lerna run --scope @frontend/bf-handler --stream build",
    "ks-empl:dev": "npx lerna run --scope @frontend/employer --stream dev",
    "bf-applicant:dev": "npx lerna run --scope @frontend/applicant --stream dev",
    "bf-handler:dev": "npx lerna run --scope @frontend/bf-handler --stream dev",
    "ks-empl:start": "npx lerna run --scope  @frontend/employer --stream start",
    "bf-applicant:start": "npx lerna run --scope  @frontend/applicant --stream start",
    "bf-handler:start": "npx lerna run --scope  @frontend/bf-handler --stream start",
    "release": "cross-env HUSKY_BYPASS=true && lerna publish",
    "husky:pre-commit": "npx lerna run --concurrency 1 --stream pre-commit",
    "lint": "npx lerna run lint",
    "test": "npx lerna run test --stream",
    "test:debug": "npx lerna run test:debug",
    "test:staged": "npx lerna run test:staged",
    "test:coverage": "npx lerna run test:coverage --stream --concurrency 1",
    "bf-applicant:test": "yarn --cwd benefit/applicant test",
    "bf-handler:test": "yarn --cwd benefit/bf-handler test",
    "ks-empl:test": " yarn --cwd kesaseteli/employer test",
    "ks-empl:browser-test": "yarn --cwd kesaseteli/employer browser-test",
    "ks-empl:browser-test:ci": "yarn --cwd kesaseteli/employer browser-test:ci"
  },
  "devDependencies": {
    "@axe-core/react": "^4.1.1",
