{
  "name": "@frontend/employer",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "PORT=3000 node start.js",
    "build": "next build",
    "start": "NODE_ENV=production PORT=3000 node start.js ",
    "lint": "eslint --ext js,ts,tsx src browser-tests",
    "pre-commit": "lint-staged -c ../../.lintstagedrc.js",
    "test": "jest --runInBand --no-cache",
    "test:debug-nock": "cross-env DEBUG=nock.* yarn test",
    "test:debug-dom": "cross-env DEBUG_PRINT_LIMIT=1000000 yarn test",
    "test:staged": "yarn test --watchAll=false --findRelatedTests",
    "test:coverage": "yarn test:debug-dom --verbose --coverage",
    "browser-test": "testcafe \"chrome --window-size='1249,720'\" testcafe.dotenv.js browser-tests/",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1249,720'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report --reporter spec,custom,html:report/index.html testcafe.dotenv.js browser-tests/"
  },
  "dependencies": {
    "@frontend/shared": "*",
    "@frontend/kesaseteli-shared": "*",
    "@sentry/browser": "^6.14.3",
    "@types/next": "^9.0.0",
    "@types/react": "^17.0.28",
    "@types/react-dom": "^17.0.6",
    "axios": "^0.24.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.13.2",
    "dotenv": "^9.0.0",
    "esm": "^3.2.25",
    "graphql": "^15.5.0",
    "hds-react": "^1.6.0",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^8.10.0",
    "next-plugin-custom-babel-config": "^1.0.5",
    "next-transpile-modules": "^8.0.0",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-loading-skeleton": "^2.2.0",
    "react-query": "^3.33.0",
    "react-toastify": "^8.0.3",
    "react-use": "^17.3.1",
    "rexreplace": "^7.0.3",
    "styled-components": "^5.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "eslint-config-adjunct": "^4.11.1"
  }
}
